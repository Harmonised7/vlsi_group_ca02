             LOAD s9, 80         ; mask 0b10000000
       main: 
             INPUT s4, 00        ;   Inputs 2 - HZ control
             AND s4, s9          ;   Mask s4 to 0b10000000
             JUMP NZ, mode_1
             JUMP mode_2
             JUMP main           ;   mode_1 and mode_2 jump to main regardless, but just in case.
             ;1hz
     mode_1: 
             CALL turn_on
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL turn_off
             CALL start_delay
             JUMP main
             ;10hz
     mode_2: 
             CALL turn_on
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
             CALL start_delay
